<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
    <!-- The identifier that must be unique within the hosting gallery -->
    <id>BreanosIdentityProvider</id>

    <!-- The package version number that is used when resolving dependencies -->
    <version>0.0.8</version>

    <!-- Authors contain text that appears directly on the gallery -->
    <authors>Kellner, Krisch</authors>

    <!-- Owners are typically nuget.org identities that allow gallery
            users to easily find other packages by the same owners.  -->
    <owners>Kellner, Krisch</owners>

    <!-- License and project URLs provide links for the gallery -->
    <licenseUrl>http://bre-tfs02:8080/tfs/Daipan/_git/AccessControl/BreanosIdentityProvider/</licenseUrl>
    <projectUrl>http://bre-tfs02:8080/tfs/Daipan/_git/AccessControl/BreanosIdentityProvider/</projectUrl>

    <!-- The icon is used in Visual Studio's package manager UI -->
    <iconUrl>file://smileBig.png</iconUrl>

    <!-- If true, this value prompts the user to accept the license when
            installing the package. -->
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <!-- Any details about this particular release -->
    <releaseNotes>Fixed bug in GetUserGroups group-linkage-logic</releaseNotes>

    <!-- The description can be used in package manager UI. Note that the
            nuget.org gallery uses information you add in the portal. -->
    <description>Breanos Identity Provider implementation</description>

    <!-- Copyright information -->
    <copyright>Copyright ©2018 Breanos GmbH</copyright>

    <!-- Tags appear in the gallery and can be used for tag searches -->
    <tags>identity provider</tags>

    <!-- Dependencies are automatically installed when the package is installed -->
      <dependencies>
        <dependency id="IdentityProvider.Interfaces" version="0.0.5" />
      </dependencies>
    </metadata>

    <!-- A readme.txt to display when the package is installed -->
    <files>
      <file src="BreanosIdentityProvider.dll" target="lib/netstandard2.0"/>
      <file src="BreanosIdentityProvider.pdb" target="lib/netstandard2.0"/>
      <file src="IdentityProviderModel.dll" target="lib/netstandard2.0"/>
      <file src="IdentityProviderModel.pdb" target="lib/netstandard2.0"/>
        <!--<file src="readme.txt" target="" />-->
    </files>
</package>